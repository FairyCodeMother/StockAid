<% content_for :title, "Create Revenue Stream" %>

<% content_for :content_size, "col-sm-9 col-md-10" %>

<% content_for :content do %>
  <%= form_for @revenue_stream, data: { guarded: true } do |f| %>
    <%= render partial: "revenue_streams/revenue_stream_fields", locals: { fields: f, record: @revenue_stream } %>
    <%= f.submit "Save", class: "btn btn-primary" %>
    <%= link_to "Cancel", revenue_streams_path, type: "button", class: "btn btn-default" %>
  <% end %>
<% end %>
